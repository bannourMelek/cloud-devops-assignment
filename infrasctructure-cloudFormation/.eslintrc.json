{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "root": true,
    "rules": {
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "no-console": 1,
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto",
                "singleQuote": true
            }
        ]
    }
}
